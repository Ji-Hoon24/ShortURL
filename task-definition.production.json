{
  "family": "shorti_rest_production",
  "executionRoleArn": "arn:aws:iam::224460675860:user/github-action",
  "containerDefinitions": [
    {
      "cpu": 1,
      "image": "224460675860.dkr.ecr.ap-northeast-2.amazonaws.com/shorti/shorti_rest:latest",
      "memory": 1024,
      "name": "buddystock_rest",
      "secrets": [
        {
          "valueFrom": "SSM example",
          "name": "example"
        }
      ],
      "entryPoint": ["java", "-jar", "app.jar", "--spring.profiles.active=product"],
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": "7777"
        }
      ],
      "dockerLabels": {
        "PROMETHEUS_EXPORTER_PORT": "7777"
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group" : "/ecs/production/shorti/shorti_rest",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}